<%= form_with(url: "/users", method: "post") do |form| %>
    <%= label_tag(:username, "Name:") %>
    <%= form.text_field :username %>
     <%= label_tag(:email, "Email:") %>
      <%= form.email_field :email %>
       <%= label_tag(:password, "Password:") %>
        <%= form.password_field :password %>
    <%= submit_tag("Submit") %>
  <% end %>
